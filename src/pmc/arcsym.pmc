pmclass ArcSym
  extends ArcT
  need_ext
  dynpmc
  group primitivearc_group
  hll Arc {

	ATTR STRING *repr;

	VTABLE void init() {
		Parrot_ArcSym_attributes *u = mem_allocate_zeroed_typed(Parrot_ArcSym_attributes);
		PMC_data(SELF) = u;
	}

	METHOD to_string() {
		STRING *res = PARROT_ARCSYM(SELF)->repr;
		RETURN(STRING *res);
	}

	METHOD set_repr(STRING *repr) {
		PARROT_ARCSYM(SELF)->repr = repr;
	}

}
