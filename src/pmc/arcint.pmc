pmclass ArcInt 
  extends ArcT
  extends Integer
  dynpmc
  group primitivearc_group
  hll Arc
  maps Integer {

	METHOD pr_repr() {
		const INTVAL i = VTABLE_get_integer(INTERP, SELF);
		STRING *res = Parrot_sprintf_c(INTERP, "%d", i);
		RETURN(STRING *res);
	}

	METHOD to_string() {
		const INTVAL i = VTABLE_get_integer(INTERP, SELF);
		STRING *res = Parrot_sprintf_c(INTERP, "%d", i);
		RETURN(STRING *res);
	}

	VTABLE INTVAL get_integer() {
		return Parrot_Integer_get_integer(INTERP, SELF);
	}

	VTABLE FLOATVAL get_number() {
		return Parrot_Integer_get_number(INTERP, SELF);
	}

	VTABLE STRING* name() {
		return Parrot_str_new_constant(INTERP, "int");
	}

}
