pmclass ArcInt 
  extends ArcT
  extends Integer
  dynpmc
  group primitivearc_group
  hll Arc
  maps Integer {

	METHOD to_string() {
		const INTVAL i = SELF.get_integer();
		STRING *res = Parrot_sprintf_c(INTERP, "%d", i);
		RETURN(STRING *res);
	}

	VTABLE INTVAL get_integer() {
		return Parrot_Integer_get_integer(INTERP, SELF);
	}

}
