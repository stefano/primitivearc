pmclass ArcNum 
  extends ArcT
  extends Float
  dynpmc
  group primitivearc_group
  hll Arc
  maps Float {

	VTABLE FLOATVAL get_num() {
		return Parrot_Float_get_num(INTERP, SELF);
	}

	METHOD to_string() {
		const FLOATVAL n = SELF.get_number();
		STRING *res = Parrot_sprintf_c(INTERP, "%f", n);
		RETURN(STRING *res);
	}

}
